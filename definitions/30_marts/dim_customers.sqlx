config {
  type: "table",
  schema: "analytics_dev",
  name: "dim_customers",
  tags: ["prod"],
  bigquery: {
    clusterBy: ["customer_id"]
  }
}

select
  customer_id,
  any_value(customer_name) as full_name,
  null as email,
  "FR" as country,
  min(created_ts) as created_ts
from ${ref("stg_customers")}
group by customer_id